    "keywords": [
        "laravel",
        "validator",
        "color",
        "tylercd100"
    ],
    "homepage": "https://github.com/tylercd100/laravel-validator-color",
    "license": "MIT",  
    "authors": [
        {
            "name": "Tyler Arbon",
            "email": "tylercd100@gmail.com"
        }
    ],
    "autoload":{
